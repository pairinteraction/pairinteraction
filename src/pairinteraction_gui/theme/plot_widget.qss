/* These options set the background color of the navigation toolbar icons */
QToolBar {
    background-color: #ffffff;
    color: #ffffff;
}
QToolButton {
    background-color: #ffffff;
}
QToolButton:hover {
    background-color: #909090;
}
QToolButton:checked {
    background-color: #007bff;
}
